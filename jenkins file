pipeline {
  agent any;
  stages {
    stage ("BUILD") {
      steps {
        echo "this is build stage"
      }
    }
    
     stage ("DEPLOY") {
      steps {
        echo "this is deplo stage"
      }
    }
    
     stage ("TEST") {
      steps {
        echo "this is test stage"
      }
    }
  }

}
