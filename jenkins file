pipeline {
	agent any;
		stages {
			stage{'BUILD'} {
				steps {
					echo "this is build stage"
					sh '''
						sleep 5
					   '''
				 }
				}
			stage {'test parallel'} {
				parallel {
					stage{'test in chrome'}
						steps {
							echo "this is test on chrome browser"
							sh 'sleep 5'
							
							}
						}
					stage{"test in firefox"}
						steps {
							echo "this is test on firefox browser"
							sh 'sleep 3'
							
							}
						}
				}
			}
		
